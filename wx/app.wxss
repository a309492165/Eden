/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 分割块 */
.garyBox{
  background-color: rgba(211, 211, 211, 0.6);
  width: 100%;
  height:20rpx;
}
/* 单行超出范围截取添加省略号 */
.ell{
  overflow:hidden;/* 超出范围隐藏 */
  white-space:nowrap;
  text-overflow:ellipsis;
}